{
    "name": "kleinkram",
    "version": "0.44.0",
    "main": "index.js",
    "repository": "git@github.com:leggedrobotics/GrandTourDatasets.git",
    "author": "Johann Schwabe <johann@schwabe.dev>",
    "license": "MIT",
    "scripts": {
        "start": "docker compose up --build -d",
        "eslint": "(TIMING=1 yarn run tsc:check); (TIMING=1 eslint --fix .)",
        "eslint-full": "(TIMING=1 yarn run tsc:check); (TIMING=1 node --max-old-space-size=4096 ./node_modules/.bin/eslint -c eslint-full.config.mjs --fix .)",
        "eslint:check-summary": "T(TIMING=1 yarn run tsc:check); (TIMING=1 eslint -f summary .)",
        "eslint-full:check-summary": "T(TIMING=1 yarn run tsc:check); (TIMING=1 eslint -c eslint-full.config.mjs -f summary .)",
        "eslint:check": "(TIMING=1 yarn run tsc:check); (TIMING=1 eslint -f summary .)",
        "eslint-full:check": "(TIMING=1 yarn run tsc:check); (TIMING=1 eslint -c eslint-full.config.mjs -f summary .)",
        "eslint:quiet": "(TIMING=1 yarn run tsc:check); (TIMING=1 eslint --quiet --fix .)",
        "eslint-full:quiet": "(TIMING=1 yarn run tsc:check); (TIMING=1 eslint -c eslint-full.config.mjs --quiet --fix .)",
        "tsc:check": "(tsc --noemit   --project  backend/tsconfig.json); (tsc --noemit   --project  queueConsumer/tsconfig.json); (tsc --noemit   --project  common/tsconfig.json); (vue-tsc --noEmit  --project  frontend/tsconfig.json)",
        "prettier": "prettier --write .",
        "prettier:check": "prettier --check .",
        "bump": "grunt bumpVersion",
        "bump:patch": "grunt bumpVersion:patch",
        "bump:minor": "grunt bumpVersion:minor",
        "bump:major": "grunt bumpVersion:major",
        "validateVersions": "grunt validateVersions",
        "postinstall": "(cd backend && yarn install --ignore-engines ); (cd frontend && yarn install --ignore-engines ); (cd queueConsumer && yarn install --ignore-engines ); (cd common && yarn install --ignore-engines ); (cd docs && yarn install --ignore-engines )"
    },
    "devDependencies": {
        "@eslint/js": "^9.20.0",
        "@types/eslint__js": "^8.42.3",
        "@typescript-eslint/eslint-plugin": "^8.8.1",
        "@typescript-eslint/parser": "^8.8.1",
        "@vue/eslint-config-typescript": "^14.1.3",
        "eslint": "^9.20.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-formatter-summary": "^1.1.0",
        "eslint-plugin-file-progress": "^3.0.1",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-unicorn": "^57.0.0",
        "eslint-plugin-vue": "^9.27.0",
        "grunt": "^1.6.1",
        "grunt-bump": "^0.8.0",
        "prettier": "^3.3.3",
        "typescript": "5.7.3",
        "typescript-eslint": "^8.8.1",
        "vue-eslint-parser": "^9.4.3",
        "vue-tsc": "^2.1.10"
    },
    "dependencies": {
        "class-transformer": "^0.5.1",
        "fs-extra": "^11.2.0",
        "minimatch": "8",
        "qs": "^6.13.1"
    },
    "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
